{
  "version": 2,
  "builds": [
    {
      "src": "client/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "server/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/app.js"
    },
    {
      "src": "/",
      "dest": "client/app/page"
    },
    {
      "src": "/book-a-chef",
      "dest": "client/app/book-a-chef/page"
    },
    {
      "src": "/chefs/[id]",
      "dest": "client/app/chefs/[id]/page"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "client/app/$1/page"
    }
  ]
}